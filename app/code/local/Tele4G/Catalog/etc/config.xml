<?xml version="1.0"?>
<config>
    <modules>
        <Tele4G_Catalog>
            <version>0.0.26</version>
        </Tele4G_Catalog>
    </modules>
    <global>
        <helpers>
            <tele4G_catalog>
                <class>Tele4G_Catalog_Helper</class>
            </tele4G_catalog>
        </helpers>
        <models>
            <tele4G_catalog>
                <class>Tele4G_Catalog_Model</class>
                <resourceModel>tele4G_catalog_resource</resourceModel>
            </tele4G_catalog>
            <catalog_resource>
                <rewrite>
                    <product_attribute_backend_media>Tele4G_Catalog_Model_Resource_Product_Attribute_Backend_Media</product_attribute_backend_media>
                </rewrite>
            </catalog_resource>
            <directory>
                <rewrite>
                    <currency>Tele4G_Catalog_Model_Currency</currency>
                </rewrite>
            </directory>
            <core>
                <rewrite>
                    <locale>Tele4G_Catalog_Model_Locale</locale>
                </rewrite>
            </core>
            <catalog>
                <rewrite>
                    <product_api_v2>Tele4G_Catalog_Model_Product_Api_V2</product_api_v2>
                </rewrite>
            </catalog>
        </models>
        <resources>
            <tele4G_catalog_setup>
                <setup>
                    <module>Tele4G_Catalog</module>
                    <class>Mage_Catalog_Model_Resource_Setup</class>
                </setup>
            </tele4G_catalog_setup>
        </resources>
        <blocks>
            <tele4G_catalog>
                <class>Tele4G_Catalog_Block</class>
            </tele4G_catalog>
            <catalog>
                <rewrite>
                    <product_view_type_configurable>Tele4G_Catalog_Block_Product_View_Type_Configurable</product_view_type_configurable>
                </rewrite>
            </catalog>
        </blocks>
        <events>
            <controller_action_layout_load_before>
                <observers>
                    <attributesethandle>
                        <class>tele4G_catalog/observer</class>
                        <method>addAttributeSetHandle</method>
                    </attributesethandle>
                </observers>
            </controller_action_layout_load_before>
        </events>
    </global>
</config>
