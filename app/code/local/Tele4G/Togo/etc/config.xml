<?xml version="1.0"?>
<config>
    <modules>
        <Tele4G_Togo>
            <version>0.0.1</version>
        </Tele4G_Togo>
    </modules>
    <global>
        <helpers>
            <tele4G_togo>
                <class>Tele4G_Togo_Helper</class>
            </tele4G_togo>
        </helpers>
        <models>
            <tele4G_togo>
                <class>Tele4G_Togo_Model</class>
            </tele4G_togo>
        </models>
    </global>
    <frontend>
        <routers>
            <togo>
                <use>standard</use>
                <args>
                    <module>Tele4G_Togo</module>
                    <frontName>togo</frontName>
                </args>
            </togo>
        </routers>
    </frontend>
    <default>
        <carriers>
            <togo>
                <wsdl>http://10.156.150.119:8080/comviq-order-ejb/ComviqPODOrderWS/ComviqPODOrderWS</wsdl>
                <!--<wsdl>http://angliatest2.tele2.se/proxy2/ComviqPODOrderWS?wsdl</wsdl>-->
                <title>ToGo</title>
                <name>ToGo</name>
                <specificerrmsg>
                    This shipping method is currently unavailable.
                    If you would like to ship using this shipping
                    method, please contact us.
                </specificerrmsg>
                <model>tele4G_togo/carrier_togo</model>
                <allow_addons>0</allow_addons>
                <allow_insurance>0</allow_insurance>
            </togo>
        </carriers>
    </default>
</config>
